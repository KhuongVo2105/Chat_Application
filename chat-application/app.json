{
    "expo": {
      "name": "instagram",
      "slug": "instagram",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icons--instagram.png",
      "userInterfaceStyle": "light",
      "splash": {
        "image": "./assets/instagram_splash.jpeg",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "scheme": "sora",
      "assetBundlePatterns": ["**/*"],
      "ios": {
        "supportsTablet": true
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/icons--instagram.png",
          "backgroundColor": "#ffffff"
        },
        "package": "com.jsm.sora"
      },
      "web": {
        "favicon": "./assets/favicon.png"
      },
      "plugins": ["expo-router", "expo-font"]
    }
  }